# Example secrets file - DO NOT commit actual secrets
# Create this file with: kubectl create secret generic vapi-secrets --from-env-file=secrets.env -n vapi

apiVersion: v1
kind: Secret
metadata:
  name: vapi-secrets
  namespace: vapi
type: Opaque
stringData:
  VAPI_API_KEY: "your-vapi-api-key"
  KEYCLOAK_ADMIN_PASSWORD: "your-keycloak-password"
  POSTGRES_PASSWORD: "your-postgres-password"
  MONGO_PASSWORD: "your-mongo-password"
  SERVICE_AUTH_TOKEN: "your-service-token"

